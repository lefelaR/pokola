{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/Api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/Api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/Api",
  "projects": {
    "Api": {
      "type": "application",
      "root": "apps/Api",
      "entryFile": "main",
      "sourceRoot": "apps/Api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/Api/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "driver": {
      "type": "application",
      "root": "apps/driver",
      "entryFile": "main",
      "sourceRoot": "apps/driver/src",
      "compilerOptions": {
        "tsConfigPath": "apps/driver/tsconfig.app.json"
      }
    },
    "logging-service": {
      "type": "application",
      "root": "apps/logging-service",
      "entryFile": "main",
      "sourceRoot": "apps/logging-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/logging-service/tsconfig.app.json"
      }
    },
    "logs": {
      "type": "application",
      "root": "apps/logs",
      "entryFile": "main",
      "sourceRoot": "apps/logs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/logs/tsconfig.app.json"
      }
    },
    "rider": {
      "type": "application",
      "root": "apps/rider",
      "entryFile": "main",
      "sourceRoot": "apps/rider/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rider/tsconfig.app.json"
      }
    },
    "rider-service": {
      "type": "application",
      "root": "apps/rider-service",
      "entryFile": "main",
      "sourceRoot": "apps/rider-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rider-service/tsconfig.app.json"
      }
    }
  }
}